{
  "name": "youtube-clone-backend",
  "version": "1.0.0",
  "description": "Backend for YouTube clone application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seedDatabase.js",
    "seed:clear": "node scripts/seedDatabase.js --clear-only",
    "seed:small": "node scripts/seedDatabase.js --users=10 --videos=5",
    "seed:large": "node scripts/seedDatabase.js --users=50 --videos=15"
  },
  "keywords": [
    "youtube",
    "video",
    "streaming",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
